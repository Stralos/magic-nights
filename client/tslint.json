{
  "extends": [
    "tslint:recommended", 
    "tslint-react", 
    "tslint-config-prettier", 
    "tslint-config-airbnb"
  ],
  "rules": { 
    "import-name": false, 
    "variable-name": [ 
      true, 
      "ban-keywords" 
    ],
    "ordered-imports": false ,
    "object-literal-sort-keys": false,
    "interface-name": false
  }, 
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
